#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use pub\Command;
use Symfony\Component\Console\Application;

$application = new Application('pub', '@package_version@');
$application->add(new pub\Command\HelloCommand());
$application->add(new pub\Command\GitInitCommand());
$application->add(new pub\Command\ValidateComposerCommand());
$application->run();
