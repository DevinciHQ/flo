{
  "name": "nbcuots/pub",
  "description": "CLI app for Publisher7",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric J. Duran",
      "email": "eric.duran@nbcuni.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/NBCUOTS/pub/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ericduran/php-github-api"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "4.*",
    "herrera-io/phar-update": "~2.0",
    "symfony/console": "~2.5",
    "symfony/filesystem": "2.6.*@dev",
    "symfony/process": "2.6.*@dev",
    "symfony/yaml": "~2.6",
    "illuminate/support": "4.3.*@dev",
    "illuminate/filesystem": "4.3.*@dev",
    "teqneers/php-stream-wrapper-for-git": "dev-master",
    "knplabs/github-api": "dev-deploymentapi",
    "vierbergenlars/php-semver": "3.0.0"
  },
  "bin": [
    "pub"
  ],
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-0": {
      "pub": "src"
    }
  }
}
